{
    "name": "<%= _.slugify(appname) %>",
    "version": "0.0.0",
    "main": "index.html",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components"
    ],
    "appPath": "app",
    "dependencies": {
        "jquery": "~2.0",
        <% if(uirouter) { print('"angular-ui-router": "~0.2",'); } else { print('"angular-route": "~1.4.6",'); } %>
        <% if(sass) { print('"bootstrap-sass-official": "~3.3.5",'); } else { print('"less.js": "~1.6",'); } %>
        <% if(sass) { print('"angular-bootstrap": "~0.11",'); } %>
        "lodash": "~3.10.1",
        "bootstrap": "~3.3.5",
        "angular": "~1.4.6",
        "angular-aria": "~1.4.6",
        "angular-animate": "~1.4.6",
        "angular-resource": "~1.4.6",
        "angular-cookies": "~1.4.6",
        "angular-translate": "~2.5.2",
        "angular-translate-loader-static-files": "~2.5.2",
        "angular-translate-loader-url": "~2.5.2",
        "angular-mocks": "~1.4.6",
        "angular-ui-utils": "~0.1",
        "angular-bootstrap": "~0.11",
        "moment": "~2.5",
        "font-awesome": "~4.0"
    },
    "devDependencies": {
        "angular-mocks": "^1.3.0",
        "font-awesome": "~4.0",
        "jasmine-jquery": "~2.1.1"
    }
}
